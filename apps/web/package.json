    "@tailwindcss/vite": "^4.1.14",
    "@types/node": "^22",
    "@ws-kit/zod": "^0.3.4",
    "eslint": "^9.39.1",
    "prettier": "^3.6.0",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "svelte": "^5.43.5",
    "svelte-check": "^4.3.0",
    "tailwindcss": "^4.1.14",
